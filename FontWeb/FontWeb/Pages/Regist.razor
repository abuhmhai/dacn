<div class="container mt-5" style="border: solid 1px; width: 400px">
    <div class="col">
        <h2 class="text-center mt-3 mb-4">Nạp VIP</h2>
        <form action="#">
            <div class="form-floating mb-3">
                <input type="email" class="form-control" id="floatingInput" placeholder="name@example.com" style="height: 1px">
                <label for="floatingInput">Email address</label>
            </div>

            <div class="form-floating mb-3">
                <p>Chọn phương thức thanh toán: </p>
            </div>

            <div class="text-center mt-2 mb-4">
                <button type="submit" class="btn btn-primary">Thanh toán</button>
            </div>
        </form>
    </div>
</div>

